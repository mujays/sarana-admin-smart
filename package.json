{
  "name": "dashboard-ai",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --port=3005",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint --ext .ts,.tsx ./src",
    "lint:check": "npx run eslint",
    "lint:fix": "npx run eslint --fix",
    "lint:staged": "eslint --fix",
    "prettier": "prettier \"**/*.+(json|ts|tsx|css|md)\"",
    "format:check": "npx run prettier --check",
    "format:fix": "npx run prettier --write",
    "format:staged": "prettier --write",
    "types:check": "tsc --noEmit --pretty",
    "types:staged": "tsc --noEmit --pretty",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "e2e:open": "./node_modules/.bin/cypress open",
    "e2e:run": "./node_modules/.bin/cypress run",
    "e2e:run:headed": "./node_modules/.bin/cypress run --headed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.5",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/static-style-extract": "^1.0.1",
    "@tanstack/react-query": "^5.59.20",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@types/uuid": "^9.0.1",
    "antd": "^5.4.6",
    "apexcharts": "^4.7.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "csstype": "^3.1.2",
    "dayjs": "^1.11.13",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "firebase": "^11.10.0",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.454.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "next": "13.4.1",
    "next-progress": "^2.3.1",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "18.2.0",
    "sonner": "^1.5.0",
    "typescript": "5.0.4",
    "uuid": "^9.0.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/js-cookie": "^3.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "local-cypress": "^1.2.6",
    "next-router-mock": "^0.9.3",
    "postcss": "^8.4.47",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.0.9",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0"
  }
